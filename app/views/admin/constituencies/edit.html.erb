<div id="overview" class="left-column">
	<div class="left-content">
  <%= content('edit-constituency') %>
  <%= link_button_to 'Constituency Administration', admin_constituencies_path, :size => 'seventy', :class => 'btn blue-btn' %> 
</div>
</div><!--overview-->

<div id="details" class="right-column">
	<div class="the-content">
  <%= form_for [:admin,@constituency] do |f| %>
    <%= render(:partial => 'form', :locals => { :f => f } ) %>
    <div class='field field-submit'>
    	<%= f.submit "Update Constituency", :class => 'btn blue-btn' %>
    </div>
  <% end %>
</div>
</div><!--details-->
