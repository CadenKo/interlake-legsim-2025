
  <div id="overview" class="left-column">
    <div class="left-content">
    <%= content('edit-tutorial') %>
    <%= link_button_to "Tutorial Management", admin_tutorials_path, :size => 'hundred', :class => 'btn blue-btn' %>
    </div>
  </div>
  <div id="details" class="right-column">
      <div class="the-content">
      <%= form_for @tutorial, :url => { :action => :create } do |f| %>
        <%= render(:partial => 'form', :locals => { :f => f } ) %>
        <div class='field field-submit'>
          <%= submit_tag 'Create Tutorial', :class => 'btn blue-btn' %>
        </div>
      <% end %>
    </div>
  </div>