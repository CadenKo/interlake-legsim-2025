
<div id="overview" class="left-column">
  <div class="left-content">
    <%= content('vote-administration') %>

    <%= link_button_to "Return to Administration", admin_path, :size => 'seventy' %><br />
  </div>
</div>

<div id="details" class="right-column">
  <div class="the-content">
    <div class="expander expanded">
    <div class="bar clearfix">
      <a href="#" class="title">Floor Votes</a>
    </div><!--bar-->
    <div class="expander-contents">
      <table cellspacing="0">
        <tr>
          <th>Legislation</th>
        </tr>
        <% @floor.votes.includes( motion: { referral: :legislation } ).each do |vote| %>
        <tr>
          <td><%= vote.motion.referral.legislation.reference %></td>
          <td>
        </tr>
        <% end %>
      </table>
    </div><!--expander-contents-->
    </div>
  </div>