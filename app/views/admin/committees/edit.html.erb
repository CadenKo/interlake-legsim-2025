<div id="overview" class="left-column">
  <div class="left-content">
    <%= content('admin-edit-committee') %>

    <%= link_button_to 'Return to Administration', admin_path, :size => 'hundred', :class => 'btn blue-btn' %>
  </div>
</div><!--overview-->

<div id="details" class="right-column">
  <div class="the-content">
    <%= form_for [:admin,@committee] do |f| %>
      <%= render(:partial => 'form', :locals => { :f => f } ) %>
      <div class='field field-submit'>
        <%= submit_tag("Update Committee", :class => 'btn blue-btn') %>
      </div>
    <% end %>
  </div>
</div><!--details-->

