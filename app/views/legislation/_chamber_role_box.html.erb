  <div id="legislation" class="expander expanded">
    <div class="bar clearfix">
      <h2 href="#" class="title">Legislation <span class="parenthesis"></span></h2>
      <div class="small-link"></div>
    </div><!--bar-->
    <div class="expander-contents">
    <table cellspacing="0">
      <thead>
        <tr>
          <th colspan="4">Sponsored Legislation [<a href="/legislation/new" title="Introduce New Legislation">Introduce New Legislation</a>]</th>
        </tr>
      </thead>
      <tbody>
        <% chamber_role.sponsored_legislation.active.each do |legislation| %>
          <tr>
            <td class="<%= legislation.status %>"><%= link_to_legislation( legislation ) %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
    <table cellspacing="0">
      <thead>
        <tr>
          <th colspan="4">Cosponsored Legislation [<a href="/legislation" title="Search Legislation Database">Search Legislation Database</a>]</th>
        </tr>
      </thead>
      <tbody>
        <% chamber_role.cosponsored_legislation.active.each do |legislation| %>
          <tr>
            <td class="<%= legislation.status %>"><%= link_to_legislation( legislation ) %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
