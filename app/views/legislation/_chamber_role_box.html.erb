    <table cellspacing="0" class="legend">

      <thead>
          <tr>
            <th colspan="7">Status</th>
          </tr>
      </thead>
      <tbody>
          <tr>
            <td class="introduced">Introduced   </td>
            <td class="referred">Referred</td>
            <td class="calendar">Calendar</td>
            <td class="passed">Passed</td>
            <td class="failed">Failed</td>
            <td class="vetoed">Vetoed</td>
            <td class="law">Law</td>
          </tr>
      </tbody>
