
<div id="overview" class="left-column">
  	<div class="left-content">
    <%= content('submit-legislation') %>
</div>
  </div><!--overview-->

  <div id="details" class="right-column">
  	<div class="the-content">
    <%= form_for @legislation, :url => legislation_path(@legislation) do |f| %>
      <%= render(:partial => 'form', :locals => { :f => f } ) %>
      <div class='field field-submit'>
        <%= submit_tag('Update Legislation', :size => 'forty', :class => 'btn blue-btn' ) %>
      </div>
    <% end %>

  </div><!--details-->
</div>