<h1>Course</h1>

<%= @course.title %><br />

<p>
  <% @course.chambers.each do |chamber| %>
    <%= chamber.title %> :
    <%= link_to "show", chamber_path( chamber ) %> :
    <%= link_to "edit", edit_chamber_path( chamber ) %> :
    <%= link_to link_to "delete", { :action => "destroy", :id => chamber },
      :confirm => "Are you sure you want to delete this chamber?",
      :method => :delete %>
    <br />
  <% end %>

  <p><%= link_to "Create New Chamber for #{@course.title}", new_course_chamber_path( @course ) %><br /></p>

</p>


<%= link_to "Back To Course Index", courses_path %>